<!--contain-->
<div class="container">
  {{#with zodiac}}
    <h1 class="text-center">{{name}} Characteristics</h1>
    <div class="container-fluid well span6">
      <div class="row">
        <div class="col-md-4">
          <img src="/images{{imagePath}}" class="img-circle" />
        </div>
        <div class="col-md-8">
          <h3>{{name}}</h3>
          <h4>Strengths: <small>{{strengths}}</small></h4>
          <h4>Weaknesses:
            <small>{{weaknesses}}</small>
          </h4>
          <h4>Likes: <small>{{likes}}</small></h4>
          <h4>Dislikes: <small>{{dislikes}}</small></h4>
          <h4><small>{{description}}</small></h4>
        </div>
      </div>
    </div>
  {{/with}}
</div>

<!--/contain-->